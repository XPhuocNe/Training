// Link problem 1: https://oj.vnoi.info/problem/bedao_g04_refi
// Code solution:

#include <bits/stdc++.h>
#define NAME "XPhuoc"

using namespace std;

#define X first
#define Y second
#define pb push_back
#define Fort(i, a, b) for (int i = (a), _b = (b); i <= _b; i++)
#define Ford(i, a, b) for (int i = (a), _b = (b); i >= _b; i--)

typedef pair<int, int> pairII;
typedef pair<int, long long> pairIL;

const int MOD = 998244353;

template <typename T>
    void total(T &x, T y)
    {
        x += y;
        x %= MOD;

        x += MOD;
        x %= MOD;
    }

/*..............................................................................................................................................*/

int n;
string S;

void Read()
{
    cin >> S;

    n = S.size();
    S = ' ' + S;
}

void init()
{

}

void Solve()
{
    vector<long long> dp(20, 0);
    vector<long long> pt(20, 0);

    dp[0] = 1;

    int PO = 1;
    int m = 0;

    Ford(i, n, 1)
    {
        if (S[i] == '?')
        {
            swap(pt, dp);

            Fort(r, 0, 16)
            {
                dp[r] = 0;

                Fort(c, 0, 9)
                    total(dp[r], pt[((r - c * PO) % 17 + 17) % 17]);
            }
        }

        if (S[i] != '?')
        {
            m += PO * (S[i] - '0');
            m %= 17;
        }

        PO *= 10;
        PO %= 17;
    }

    cout << dp[((5 - m) % 17 + 17) % 17] << "\n";
}
int main()
{
    ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);

    #ifndef ONLINE_JUDGE
        freopen(NAME".INP", "r", stdin);
        freopen(NAME".OUT", "w", stdout);
    #endif // ONLINE_JUDGE

    int TEST = 1;

    if (TEST == 0)
        cin >> TEST;

    while (TEST--)
    {
        Read();
        init();
        Solve();
    }

    return 0;
}

