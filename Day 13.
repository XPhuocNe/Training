// Link problem 2: https://oj.vnoi.info/problem/bedao_g04_regra
// Code solution:

#include <bits/stdc++.h>
#define NAME "XPhuoc"

using namespace std;

#define X first
#define Y second
#define pb push_back
#define Fort(i, a, b) for (int i = (a), _b = (b); i <= _b; i++)
#define Ford(i, a, b) for (int i = (a), _b = (b); i >= _b; i--)

typedef pair<int, int> pairII;
typedef pair<int, long long> pairIL;

const int MOD = 1e9 + 7;

template <typename T>
    void total(T &x, T y)
    {
        x += y;
        x %= MOD;

        x += MOD;
        x %= MOD;
    }

/*..............................................................................................................................................*/

const int N = 1e5;
int n;
int a[N + 7];
int d[N + 7];
vector<int> g[N + 7];
int m;

int q;

void Read()
{
    cin >> n >> m >> q;

    Fort(i, 1, m)
    {
        int u, v;
        cin >> u >> v;

        g[u].push_back(v);
        g[v].push_back(u);
    }
}

void init()
{
    Fort(u, 1, n)
    {
        a[u] = 0;
        d[u] = -1;
    }
}

void update(int u, int range, int x)
{
    queue<int> q;

    d[u] = 0;
    q.push(u);

    vector<int> nd;
    nd.push_back(u);
    a[u] = max(a[u], x);

    while (!q.empty())
    {
        u = q.front();
        q.pop();

        if (d[u] == range)
            continue;

        for(int v : g[u])
            if (d[v] < 0)
            {
                d[v] = d[u] + 1;
                q.push(v);
                a[v] = max(a[v], x);
                nd.push_back(v);
            }
    }

    for(int v : nd)
        d[v] = -1;

}

void Solve()
{
    while (q--)
    {
        int u, d, x;
        cin >> u >> d >> x;

        update(u, d, x);
    }

    Fort(i, 1, n)
        cout << a[i] << " ";
}
int main()
{
    ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);

    #ifndef ONLINE_JUDGE
        freopen(NAME".INP", "r", stdin);
        freopen(NAME".OUT", "w", stdout);
    #endif // ONLINE_JUDGE

    int TEST = 1;

    if (TEST == 0)
        cin >> TEST;

    while (TEST--)
    {
        Read();
        init();
        Solve();
    }

    return 0;
}

