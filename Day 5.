// Link problem 1: https://oj.vnoi.info/problem/bedao_r20_a
// Code solution:
#include <bits/stdc++.h>
#define NAME "XPhuoc"

using namespace std;

#define X first
#define Y second
#define pb push_back
#define Fort(i, a, b) for (int i = (a), _b = (b); i <= _b; i++)
#define Ford(i, a, b) for (int i = (a), _b = (b); i >= _b; i--)

typedef pair<int, int> pairII;
typedef pair<int, long long> pairIL;

/*..............................................................................................................................................*/

long long n;

bool iSqrt(long long x)
{
    long long r = sqrt(x);

    return (r * r) == x;
}

void Read()
{
    cin >> n;
}

void init()
{

}

void Solve()
{
    if (n % 16 != 0)
    {
        cout << 0 << "\n";
        return ;
    }

    n /= 16;

    if (iSqrt(n) == 0)
    {
        cout << 0 << "\n";
        return ;
    }

    n = sqrt(n);

    long long result = 0;

    for(long long x = 1; x * x <= n; x++)
        if (n % x == 0)
        {
            result++;

            if (n / x != x)
                result++;
        }

    cout << result << "\n";
}

int main()
{
    ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);

    #ifndef ONLINE_JUDGE
        freopen(NAME".INP", "r", stdin);
        freopen(NAME".OUT", "w", stdout);
    #endif // ONLINE_JUDGE

    int TEST = 0;

    if (TEST == 0)
        cin >> TEST;

    while (TEST--)
    {
        Read();
        init();
        Solve();
    }

    return 0;
}
