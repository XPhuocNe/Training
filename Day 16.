// Link problem 2: https://oj.vnoi.info/problem/bedao_g07_password
// Code solution:

#include <bits/stdc++.h>

using namespace std;

const int INF = 1e9 + 7;

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    
    int n;
    cin >> n;
    
    int result = -INF;
    
    for(int i = 1; i <= n; i++)
    {
        int x;
        cin >> x;
        result = max(result, x);
    }
    
    cout << result << "\n";
    
    return 0;
}

