// Link problem 1: https://oj.vnoi.info/problem/bedao_g07_colours
// Code solution:

#include <bits/stdc++.h>
#define NAME "XPhuoc"

using namespace std;

#define X first
#define Y second
#define pb push_back
#define Fort(i, a, b) for (int i = (a), _b = (b); i <= _b; i++)
#define Ford(i, a, b) for (int i = (a), _b = (b); i >= _b; i--)

typedef pair<int, int> pairII;
typedef pair<int, long long> pairIL;

const long long MOD = 1e9 + 7;

template<typename T>
    void mul(T &x, T y)
    {
        x *= y;
        x %= MOD;
    }

/*..............................................................................................................................................*/

long long P(int k, int n)
{
    if (k > n)
        return 0;

    long long res = 1;

    Fort(x, n - k + 1, n)
        mul(res, 1LL * x);

    return res;
}

const int N = 1e5;

int n, k;

vector<int> g[N + 7];

long long result = 1;

void Read()
{
    cin >> n >> k;

    Fort(i, 1, n - 1)
    {
        int u, v;
        cin >> u >> v;

        g[u].push_back(v);
        g[v].push_back(u);
    }
}

void DFS(int u, int par)
{
    for(int v : g[u])
        if (v != par)
            DFS(v, u);

    if (par == 0)
    {
        mul(result, 1LL * k);

        mul(result, P(g[u].size(), k - 1));
    } else mul(result, P(g[u].size() - 1, k - 2));
}

void init()
{

}

void Solve()
{
    result = 1;

    DFS(1, 0);

    cout << result << "\n";
}
int main()
{
    ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);

    #ifndef ONLINE_JUDGE
        freopen(NAME".INP", "r", stdin);
        freopen(NAME".OUT", "w", stdout);
    #endif // ONLINE_JUDGE

    int TEST = 1;

    if (TEST == 0)
        cin >> TEST;

    while (TEST--)
    {
        Read();
        init();
        Solve();
    }

    return 0;
}
